@model IEnumerable<Schemes.ViewModels.SchemeDetails>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SchemesDetailID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NameOftheScheme)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EligibilityCriteria)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Benefits)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Area)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DocumentsRequired)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ApplyAndLink)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsActive)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SchemesDetailID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NameOftheScheme)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EligibilityCriteria)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Benefits)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Area)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DocumentsRequired)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ApplyAndLink)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsActive)
            </td>
        </tr>
}
    </tbody>
</table>
